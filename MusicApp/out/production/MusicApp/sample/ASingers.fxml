<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="600.0" prefWidth="700.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ASingersController">
   <children>
      <Label layoutX="80.0" layoutY="50.0" prefHeight="50.0" prefWidth="150.0" text="Singer">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Line endX="452.0" layoutX="180.0" layoutY="100.0" startX="-100.0" />
      <Button fx:id="singerDeleteButton" layoutX="207.0" layoutY="181.0" mnemonicParsing="false" onAction="#singerDeleteButtonAction" prefHeight="80.0" prefWidth="75.0" style="-fx-background-radius: 100;" styleClass="btnsADU" text="Delete" textFill="WHITE" />
      <Button fx:id="singerAddButton" layoutX="318.0" layoutY="141.0" mnemonicParsing="false" onAction="#singerAddButtonAction" prefHeight="80.0" prefWidth="75.0" style="-fx-background-radius: 100;" styleClass="btnsADU" text="Add" textFill="WHITE" />
      <Button fx:id="singerUpdateButton" layoutX="429.0" layoutY="181.0" mnemonicParsing="false" onAction="#singerUpdateButtonAction" prefHeight="80.0" prefWidth="75.0" style="-fx-background-radius: 100;" styleClass="btnsADU" text="Update" textFill="WHITE" />
      <TextField fx:id="singerNameTextField" layoutX="212.0" layoutY="324.0" prefHeight="26.0" prefWidth="283.0" promptText="Singer Name" />
      <TextField fx:id="countryTextField" layoutX="211.0" layoutY="375.0" prefHeight="26.0" prefWidth="283.0" promptText="Country" />
      <Label fx:id="addLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="208.0" layoutY="522.0" prefHeight="18.0" prefWidth="283.0" textAlignment="CENTER" />
      <TextField fx:id="singerNameUpdateTextField" layoutX="212.0" layoutY="424.0" prefHeight="26.0" prefWidth="283.0" promptText="Update Singer Name" />
      <TextField fx:id="countryUpdateTextField" layoutX="211.0" layoutY="472.0" prefHeight="26.0" prefWidth="283.0" promptText="Update Country" />
   </children>
</AnchorPane>
